<po-page-default p-title="${apiName}">
  <po-dynamic-form #dynamicForm
      [p-fields]="fields"
      [p-value]="${jsonName}">
  </po-dynamic-form>
  <br>
  <div class="po-row">
      <po-button
          class="po-md-3"
          p-label="Salvar"
          [p-disabled]="dynamicForm?.form.invalid"
          (p-click)="poNotification.success('Data saved successfully!'); dynamicForm.form.reset()">
      </po-button>
  </div>
</po-page-default>